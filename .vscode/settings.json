{
    "editor.defaultFormatter": "xaver.clang-format",
    "editor.formatOnSave": true,
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "clang-format.language.cpp.enable": true,
    "clang-format.language.cpp.style": "file",
    "clang-format.assumeFilename": "${workspaceFolder}/.clang-format",
    "clang-format.executable": "C:\\Program Files\\LLVM\\bin\\clang-format.exe",
    "C_Cpp.default.cppStandard": "c++23",
    "testMate.cpp.test.executables": "${workspaceFolder}/build/vs2022-x64/Debug/unit_test_runner.exe",
    "testMate.cpp.test.workingDirectory": "${workspaceFolder}",
    "chat.tools.autoApprove": true,
    "chat.tools.terminal.autoApprove": {
        "rm": true,
        "rmdir": false,
        "del": true,
        "dir": true,
        "kill": false,
        "curl": false,
        "wget": false,
        "eval": false,
        "chmod": false,
        "chown": false,
        "git": true,
        "cmake": true,
        "/^ctest\\b/": true,
        "ls": true,
        "tree": true,
        "/^.*\\unit_test_runner.exe\\b/i": true,
        "/^.*\\level_editor.exe\\b/i": true,
        "/^.*\\build.bat\\b/i": true,
        "/^Remove-Item\\b/i": true,
        "/^Get-ChildItem\\b/i": true,
